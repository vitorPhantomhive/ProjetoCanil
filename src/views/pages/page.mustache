{{>partials/header}}

{{#banner}}
<section class="banner" style="background-image: url('images/{{background}}')">{{title}}</section>
<h2>{{title}} disponíveis para adoção</h2>
{{/banner}}

<div class="container list">
    {{#list}}
    <div class="item">
        <img src="images/{{image}}" class="item--image" />
        <div class="{{name}}">Pastor-alemão</div>
        <div class="{{color}}">Cor: Amarelo e Preto</div>
        <div class="{{sex}}">Gênero: Masculino</div>
    </div>
    {{/list}}
</div>

{{>partials/footer}}